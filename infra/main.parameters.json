{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "name": {
            "value": "${AZURE_ENV_NAME}"
        },
        "location": {
            "value": "${AZURE_LOCATION}"
        },
        "vmAdminUsername": {
            "value": "${AZURE_VM_ADMIN_USERNAME}"
        },
        "vmAdminPasswordOrKey":{
            "value": "${AZURE_VM_ADMIN_PASSWORD}"
        },
        "aiModelDeployments": {
            "value": [
                {
                  "name": "textembed",
                  "model": {
                    "name": "text-embedding-ada-002",
                    "format": "OpenAI",
                    "version": "2"
                  },
                  "sku": {
                    "name": "Standard",
                    "capacity": 10
                  }
                },
                {
                  "name": "gpt",
                  "model": {
                    "name": "gpt-4o",
                    "version": "2024-05-13",
                    "format": "OpenAI"
                  },
                  "sku": {
                    "name": "GlobalStandard",
                    "capacity": 10
                  }
                }
              ]
        }
    }
  }